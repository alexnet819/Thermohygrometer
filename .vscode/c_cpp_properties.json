{
    "configurations": [
        {
            "name": "Raspberry Pi Pico",
            "includePath": [
                "${workspaceFolder}",
                "${env:HOME}/pico-sdk/**",
                "${workspaceFolder}/src/dht11_22/inc/**",
                "${workspaceFolder}/src/lcd1602/inc/**",
                "${workspaceFolder}/src/build/generated/pico_base"
            ],
            "defines": [
                "PICO_ON_DEVICE=1"
            ],
            "intelliSenseMode": "linux-gcc-arm",
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c17",
            "cppStandard": "c++20"
        }
    ],
    "version": 4
}